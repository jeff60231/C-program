// ConsoleApplication1.cpp : 定義主控台應用程式的進入點。
//

#include "stdafx.h"
#include <stdio.h>
#include <string.h>


int main()
{
	 char *string;
	 int length[100];
	 int space = 0;
	 size_t i;

	 scanf_s("%s", &string);

	 length[0] = strlen(strtok(string, " "));
	 for (i = 1; strtok(string, " ") != '\0'; i++)
	 {
		 length[i] == strlen(strtok(NULL, " "));
	 }

}

