#include "stdafx.h"


// ConsoleApplication1.cpp : 定義主控台應用程式的進入點。
//

#include "stdafx.h"
#include "stdio.h"


int main()
{
	int ticketPrice; //the price is nuder 5000
	int seat; //the number of seat is nuder 250
	int orderRecord; // the number is under 10
	int total = 0;
	int temp;
	int i;
	puts("Please enter price(under 5000), number of seat(under 250), number of ordered record(under 10) in order  ");
	scanf_s("%d%d%d", &ticketPrice, &seat, &orderRecord);

	for (; orderRecord => 0 , orderRecord--)
	{
		scanf_s("%d", &temp);
	}
}
