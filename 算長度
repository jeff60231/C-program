#include <stdlib.h>
#include <stdio.h>
#include <string.h>

int main()
{
    char string[100];
    char  string1[100];
    int i = 0;
    int counter = 0;
    int counterS = 0;
    char * token ;
    scanf("%s", &string);
    for(i = 0; string[i] =! '\0'; i++)
    {
          counterS ++;      
    }
    token =  strtok(string, " ");
    strcpy(string1 , token);
    for(i = 0; string == '\0'; i ++)
    {
    token =  strtok(NULL, " ");
    strcpy(string1 , token);
    }
    for(i = 0; string1[i] =! '\0'; i++)
    {
         counter++;
    }
    
    printf("%f", counter / counterS ); 
}
